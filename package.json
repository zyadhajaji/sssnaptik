  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "autoprefixer": "^10.4.19",
    "css-loader": "^7.1.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
